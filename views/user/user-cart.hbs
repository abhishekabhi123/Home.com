<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Bootstrap demo</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
  <link rel="stylesheet" href="cart.css">
  <style>
    .kart{
            margin-left: 25rem !important;
    }
    .shop-kart{
            margin-left: 10rem !important;
    }
  </style>
</head>

<body>
  <span class="kart">
    Shopping Cart
  </span>
  <div class="shop-kart">
    <div class="cart-box">
      <table class="table tab">
        <thead class="tab-head">
          <tr>
            <th>Products</th>
            <th>Quantity</th>
            <th>Total</th>
          </tr>
        </thead>
        <tbody>
          {{#each orders}}
          <tr>
            <td scope="row">
              <div class="box ">
                <button type="button" onclick='removeFromCart("{{this._id}}","{{this.cart._id}}")' class="btn-close"
                  aria-label="Close"></button>
                <div class="pro-box">
                </div>

                <div class="pro-text">
                  <p>{{this.cart.name}}</p>
                </div>
              </div>
            </td>
            <td>
              {{!-- <div class="in-btn">
                <span class="next" onclick="nextNum()"> </span>
                <span class="prev" onclick="prevNum()"> </span>
                <div id="box">
                </div>
              </div> --}}
              <button type="button" id="{{this.cart._id}}{{this._id}}"
                onclick='changeQuantity("{{this._id}}","{{this.cart._id}}","{{this.userId}}","-1")'>-</button>
              <span id="{{this.cart._id}}">{{this.cartItems.quantity}}</span>
              <button type="button"
                onclick='changeQuantity("{{this._id}}","{{this.cart._id}}","{{this.userId}}","1")'>+</button>
            </td>
            <td><span class="price">{{this.cart.price}}</span></td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
    <div class="price-card">
      <span class="summary">Summary</span>
      <div class="container">
        <div class="row justify-content-between mb-3">
          <div class="col-6 ">
            <span class="total">Subtotal</span>
          </div>
          <div class="col-2 price">$ <span id="total">
            {{total}}
          </span>
          </div>
        </div>
        <div class="row justify-content-between">
          <div class="col-6">
            <span class="total"> Shipping</span>
          </div>
          <div class="col-2 price">
            Free
          </div>
        </div>

      </div>
      <hr>
      <span class="total code">Discount Code</span>
      <div class="coupons">
        <div class="coupon-code">

          <div class="input-group">
            <input required type="text" name="text" autocomplete="off" class="input">
            <label class="user-label">Coupon Code</label>
          </div>
          <button class="botn">Apply</button>
        </div>
      </div>
      <hr>
      <div class="tot d-flex justify-content-between">
        <p class="total">Total</p>
        <p class="price">$ <span id="total">
            {{total}}
          </span></p>
      </div>
      <form action="/checkout">
      
      <button class="css-button-fully-rounded--black
      button-fully-rounded--black checkoutbtn" type="submit">Proceed to checkout</button> 
      </form>

